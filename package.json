{
  "name": "comunidades-unidas-internal",
  "description": "the code for comunidades unidas internal website",
  "repository": "git@github.com:JustUtahCoders/comunidades-unidas-internal.git",
  "author": "Joel Denning",
  "license": "MIT",
  "scripts": {
    "fix-everything": "docker-compose down && docker-compose pull && yarn install",
    "start": "node backend/server.js",
    "develop": "concurrently -n w: yarn:develop-*",
    "develop-frontend": "cross-env PUBLIC_PATH='http://localhost:9018/' RUNNING_LOCALLY='true' webpack-dev-server --mode=development",
    "develop-backend": "cross-env RUNNING_LOCALLY='true' nodemon backend/server.js --watch backend",
    "develop-database": "docker-compose up",
    "build": "webpack --mode=production",
    "analyze-frontend-build": "webpack --mode=production --env.analyze=true",
    "nuke-database": "docker-compose down -v",
    "db-migrate-prod": "db-migrate up --env prod",
    "lint": "eslint frontend",
    "format": "prettier \"{frontend,__{tests,mocks}__,backend}/**/*.{js,ts,tsx}\" --write",
    "check-format": "prettier \"{frontend,__{tests,mocks}__,backend}/**/*.{js,ts,tsx}\" --check",
    "test": "jest",
    "typescript": "tsc"
  },
  "browserslist": [
    "last 5 Chrome major versions",
    "last 3 Safari major versions",
    "last 5 Firefox major versions",
    "last 2 Edge major versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn typescript && pretty-quick --staged && yarn test"
    }
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@types/jest": "^24.0.15",
    "@types/jquery": "^3.3.30",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.8.23",
    "@types/react-dom": "16.8.4",
    "axios": "^0.19.0",
    "bandicoot": "^2.10.0",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "https://github.com/joeldenning/cookie-session.git#secure-fix",
    "dateformat": "^3.0.3",
    "dayjs": "^1.8.15",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.2",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "fuzzy-search": "^3.0.1",
    "js-cookie": "^2.2.0",
    "keygrip": "^1.1.0",
    "kremling": "^2.0.1",
    "lodash-es": "^4.17.15",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "query-string": "^6.8.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "timeago.js": "^4.0.0-beta.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@zeit/ncc": "^0.20.4",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.1",
    "copyfiles": "^2.1.1",
    "cross-env": "^5.2.0",
    "db-migrate": "^0.11.6",
    "db-migrate-mysql": "^1.1.10",
    "eslint": "^6.1.0",
    "eslint-config-react-important-stuff": "^1.1.0",
    "file-loader": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^1.4.3",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
