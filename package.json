{
  "name": "comunidades-unidas-internal",
  "description": "the code for comunidades unidas internal website",
  "repository": "git@github.com:JustUtahCoders/comunidades-unidas-internal.git",
  "author": "Joel Denning",
  "license": "MIT",
  "scripts": {
    "fix-everything": "docker-compose down && docker-compose pull && yarn install",
    "start": "node backend/server.js",
    "develop": "concurrently -n w: yarn:develop-*",
    "develop-frontend": "cross-env PUBLIC_PATH='http://localhost:9018/' RUNNING_LOCALLY='true' webpack-dev-server --mode=development",
    "develop-backend": "cross-env RUNNING_LOCALLY='true' nodemon backend/server.js --watch backend",
    "develop-database": "docker-compose up",
    "build": "webpack --mode=production",
    "analyze-frontend-build": "webpack --mode=production --env.analyze=true",
    "nuke-database": "docker-compose down -v",
    "db-migrate-prod": "db-migrate up --env prod",
    "lint": "eslint frontend",
    "format": "prettier \"{frontend,__{tests,mocks}__,backend}/**/*.{js,ts,tsx}\" --write",
    "check-format": "prettier \"{frontend,__{tests,mocks}__,backend}/**/*.{js,ts,tsx}\" --check",
    "test": "jest",
    "typescript": "tsc"
  },
  "browserslist": [
    "last 5 Chrome major versions",
    "last 3 Safari major versions",
    "last 5 Firefox major versions",
    "last 2 Edge major versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently 'yarn:lint' 'yarn:typescript' 'yarn:test'"
    }
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@types/jest": "^25.1.1",
    "@types/jquery": "^3.3.31",
    "@types/lodash-es": "^4.17.3",
    "@types/reach__router": "^1.2.6",
    "@types/react-dom": "^16.9.5",
    "abortcontroller-polyfill": "^1.4.0",
    "axios": "^0.19.2",
    "bandicoot": "^4.0.0",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "https://github.com/joeldenning/cookie-session.git#secure-fix",
    "dateformat": "^3.0.3",
    "dayjs": "^1.8.19",
    "dompurify": "^2.0.7",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "fuzzy-search": "^3.1.0",
    "js-cookie": "^2.2.1",
    "juntos-por-la-salud-node-client": "^1.1.2",
    "keygrip": "^1.1.0",
    "kremling": "^2.0.2",
    "lodash-es": "^4.17.15",
    "morgan": "^1.9.1",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.0",
    "db-migrate": "^0.11.6",
    "db-migrate-mysql": "^2.1.0",
    "eslint": "^6.8.0",
    "eslint-config-ts-react-important-stuff": "^2.0.0",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^4.0.3",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "react-axe": "^3.3.0",
    "terser-webpack-plugin": "^2.3.4",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
