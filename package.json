{
  "name": "comunidades-unidas-internal",
  "description": "the code for comunidades unidas internal website",
  "repository": "git@github.com:JustUtahCoders/comunidades-unidas-internal.git",
  "author": "Joel Denning",
  "license": "MIT",
  "scripts": {
    "fix-everything": "docker-compose down && docker-compose pull && yarn install",
    "start": "node backend/server.js",
    "develop": "concurrently -n w: yarn:develop-*",
    "develop-frontend": "cross-env PUBLIC_PATH='http://localhost:9018/' webpack-dev-server --mode=development",
    "develop-backend": "cross-env RUNNING_LOCALLY='true' nodemon backend/server.js --watch backend",
    "develop-database": "docker-compose up",
    "build": "webpack --config webpack.config.js --mode=production",
    "nuke-database": "docker-compose down -v",
    "db-migrate-prod": "db-migrate up --env prod",
    "lint": "eslint frontend",
    "format": "prettier \"{frontend,__{tests,mocks}__,backend}/**/*.{js,ts,tsx}\" --write",
    "check-format": "prettier \"{frontend,__{tests,mocks}__,backend}/**/*.{js,ts,tsx}\" --check",
    "typescript": "tsc"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn typescript && pretty-quick --staged"
    }
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.5",
    "@types/react-dom": "^16.8.2",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "https://github.com/joeldenning/cookie-session.git#secure-fix",
    "dateformat": "^3.0.3",
    "dotenv": "^7.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "fuzzy-search": "^3.0.1",
    "js-cookie": "^2.2.0",
    "keygrip": "^1.0.3",
    "kremling": "^1.2.4",
    "lodash-es": "^4.17.11",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@zeit/ncc": "^0.15.2",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "db-migrate": "^0.11.5",
    "db-migrate-mysql": "^1.1.10",
    "eslint": "^5.16.0",
    "eslint-config-react-important-stuff": "^1.0.0",
    "file-loader": "^3.0.1",
    "husky": "^1.3.1",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  }
}
