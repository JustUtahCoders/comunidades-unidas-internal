    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "form-request-submit-polyfill": "^2.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-axe": "^3.4.1",
    "terser-webpack-plugin": "^3.0.2",
