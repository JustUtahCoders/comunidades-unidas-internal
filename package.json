{
  "name": "comunidades-unidas-internal",
  "description": "the code for comunidades unidas internal website",
  "repository": "git@github.com:JustUtahCoders/comunidades-unidas-internal.git",
  "author": "Joel Denning",
  "license": "MIT",
  "scripts": {
    "fix-everything": "docker-compose down && docker-compose pull && yarn install",
    "start": "node backend/server.js",
    "develop": "concurrently -n w: yarn:develop-*",
    "develop-frontend": "cross-env PUBLIC_PATH='http://localhost:9018/' RUNNING_LOCALLY='true' webpack-dev-server --mode=development",
    "develop-backend": "cross-env RUNNING_LOCALLY='true' nodemon backend/server.js --watch backend",
    "develop-database": "docker-compose up",
    "build": "webpack --mode=production --env.prod=true",
    "analyze-frontend-build": "webpack --mode=production --env.analyze=true",
    "nuke-database": "docker-compose down -v",
    "db-migrate-prod": "db-migrate up --env prod",
    "lint": "eslint frontend",
    "format": "prettier './**/*' --write",
    "check-format": "prettier './**/*' --check",
    "test": "jest",
    "typescript": "tsc"
  },
  "browserslist": [
    "last 5 Chrome major versions",
    "last 3 Safari major versions",
    "last 5 Firefox major versions",
    "last 2 Edge major versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently 'yarn:lint' 'yarn:typescript' 'yarn:test'"
    }
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "@types/jest": "^25.2.3",
    "@types/jquery": "^3.3.38",
    "@types/lodash-es": "^4.17.3",
    "@types/reach__router": "^1.3.5",
    "@types/react-dom": "^16.9.8",
    "abortcontroller-polyfill": "^1.4.0",
    "aws-sdk": "^2.687.0",
    "axios": "^0.19.2",
    "bandicoot": "^4.4.1",
    "bcrypt": "^5.0.0",
    "btoa": "^1.2.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "https://github.com/joeldenning/cookie-session.git#secure-fix",
    "dateformat": "^3.0.3",
    "dayjs": "^1.8.28",
    "dompurify": "^2.0.11",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "ext-name": "^5.0.0",
    "fuzzy-search": "^3.2.1",
    "js-cookie": "^2.2.1",
    "juntos-por-la-salud-node-client": "^1.1.2",
    "keygrip": "^1.1.0",
    "kremling": "^2.0.4",
    "lodash-es": "^4.17.15",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "timeago.js": "^4.0.2",
    "twilio": "^3.44.0",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
